    "ioredis": "^5.2.4",
    "@upstash/redis": "1.28.4"
  },
  "devDependencies": {
    "@carbon/logger": "*",
    "@carbon/tsconfig": "*",
    "@types/node": "^20.12.5",
    "dotenv": "16.3.1",
    "eslint": "^8.23.1",
    "eslint-config-carbon": "*",
    "rimraf": "^5.0.5",
